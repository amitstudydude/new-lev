
sudo bash -c "$(wget -q https://pacstall.dev/q/install -O -)"   
pacstall -I awesome-git
apt install nala awesome 
nala install network-manager light alsa-utils acpi playerctl scrot picom libplayerctl-dev redshift gpick nitrogen lxappearance
git clone -b topbar_dock https://github.com/Amitabha37377/Awful-DOTS.git
cd Awful-DOTS
mkdir ~/.themes
mkdir ~/.icons
mkdir ~/.local/share/fonts
mv ~/.config/awesome ~/.config/awesome.bak
cp -r awesome ~/.config/
cp -r Misc/fonts/* ~/.local/share/fonts/
cp -r Misc/gtk_themes/* ~/.themes/
cp -r Misc/icon_packs/* ~/.icons/
